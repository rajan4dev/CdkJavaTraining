class Calculator {
		
	public int sum(int a,int b) throws Exception {
		if(a <= 0 || b <= 0){
			Exception e = new Exception("Input value can not be zero or negative!!!");
			throw e;
		}
		return a+b;	
	}

	public int diff(int a,int b) throws Exception {
		if(a <= 0 || b <= 0){
			Exception e = new Exception("Input value can not be zero or negative!!!");
			throw e;
		}
		return a-b;	
	}
}